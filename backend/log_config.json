{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
        "simple": {
            "format": "%(asctime)s [%(levelname)s] {%(name)s} %(message)s",
            "datefmt": "%Y-%m-%dT%H:%M:%SZ"
        }
    },
    "handlers": {
        "console": {
            "class": "logging.StreamHandler",
            "level": "DEBUG",
            "formatter": "simple",
            "stream": "ext://sys.stdout"
        }
    },
    "loggers": {},
    "root": {
        "level": "INFO",
        "handlers": [
            "console"
        ]
    }
}